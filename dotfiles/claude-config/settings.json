{
  "alwaysThinkingEnabled": true,
  "spinnerTipsEnabled": false,
  "includeCoAuthoredBy": false,
  "permissions": {
    "defaultMode": "acceptEdits",
    "disableBypassPermissionsMode": "disable",
    "allow": [
      "Task",
      "Glob",
      "Grep",
      "Read",
      "Edit",
      "Write",
      "WebFetch",
      "WebSearch",
      "TodoWrite",
      "NotebookEdit",
      "SlashCommand",
      "BashOutput",
      "KillShell",
      "ExitPlanMode",
      "Bash"
    ],
    "deny": [
      "Bash(rm -rf:*)",
      "Bash(format:*)",
      "Bash(mkfs:*)",
      "Bash(dd:*)",
      "Bash(fdisk:*)",
      "Bash(parted:*)",
      "Bash(netsh:*)",
      "Bash(net user:*)",
      "Bash(net localgroup:*)",
      "Bash(reg:*)",
      "Bash(bcdedit:*)",
      "Bash(attrib:*)",
      "Bash(del /s /q:*)",
      "Read(**/.env)",
      "Read(**/secrets/**)",
      "Read(**/*.key)",
      "Read(**/*.pem)",
      "Read(**/*credentials*)",
      "Read(**/*password*)",
      "Write(**/.env)",
      "Write(**/secrets/**)",
      "Write(**/*.key)",
      "Write(**/*.pem)"
    ],
    "ask": [
      "Bash(rm -rf:*)",
      "Bash(rm -r:*)",
      "Bash(rmdir /s:*)",
      "Bash(git push --force:*)",
      "Bash(git reset --hard:*)",
      "Bash(docker system prune:*)",
      "Bash(kubectl delete:*)"
    ]
  }
}
