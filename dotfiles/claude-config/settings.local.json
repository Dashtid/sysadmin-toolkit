{
  "cleanupPeriodDays": 14,
  "permissions": {
    "allow": [
      "Bash(awk:*)",
      "Bash(find:*)",
      "Bash(echo:*)",
      "Bash(for dir in */)",
      "Bash(do echo -n \"$dir: \")",
      "Bash(done)",
      "Bash(sort:*)",
      "Bash(do du -sh \"$dir\")",
      "Bash(code:*)",
      "Bash(ls:*)",
      "Bash(cat \"$USERPROFILE/.claude/.credentials.json\")",
      "Bash(python3:*)",
      "Bash(/dev/null)"
    ],
    "deny": [],
    "ask": []
  },
  "disableAllHooks": false
}
